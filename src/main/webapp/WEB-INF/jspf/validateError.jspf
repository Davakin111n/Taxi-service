<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page language="java" contentType="text/html; charset=UTF-8"
         pageEncoding="UTF-8"
        %>

<c:if test="${errors != null}">
    <div id="error">
    <c:forEach items="${errors}" var="error">
        ${error} </ br>
    </c:forEach>
    </div>
</c:if>
